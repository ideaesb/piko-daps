<html t:type="longform" title="Create Assessment"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
      
<body>
   <t:form t:id="dap" class="appnitro">
   
   <div class="form_description">
     <h2>Pacific Region RCSD Data &amp; Products (DAPS) Worksheet</h2>
     <p>This form is used to add or update a DAPS record into Piko (Worsheet Version June 2012)</p>
   </div>		
   
   <t:errors/>

   <ul>
   
     <li id="li_code">
      <t:label class="description" for="code" />
      <t:textField t:id="code" t:validate="maxlength=255" size="15" value="entity.code" />
     </li>
     
     <li id="li_name">
      <t:label class="description" for="name" />
      <div>
       <t:textArea t:id="name" t:validate="maxlength=1024" rows="3" cols="90" value="entity.name" />
      </div>
     </li>

     
     
     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('caBox');" title="Show or Hide">Capability Area</legend>
       <div id="caBox" class="unhidden">
		<t:checkbox t:id="variability" t:value="entity.variability"  class="element checkbox" /><t:label for="variability" class="choice" />
		<t:checkbox t:id="impacts" t:value="entity.impacts"  class="element checkbox" /><t:label for="impacts" class="choice" />
       </div>
       
      </fieldset>
     </li>


     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('focusBox');" title="Show or Hide">Focus Area</legend>
       <div id="focusBox" class="unhidden">
	
		<t:checkbox t:id="water" t:value="entity.water"  class="element checkbox" /><t:label for="water" class="choice" />
		<t:checkbox t:id="coastal" t:value="entity.coastal"  class="element checkbox" /><t:label for="coastal" class="choice" />
		<t:checkbox t:id="ecosystem" t:value="entity.ecosystem"  class="element checkbox" /><t:label for="ecosystem" class="choice" />
	
	  </div>
	 </fieldset>
	</li>	


     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('regionBox');" title="Show or Hide">Region/Locale</legend>
       <div id="regionBox" class="unhidden">
	

		<t:checkbox t:id="centralNorthPacific" t:value="entity.centralNorthPacific"  class="element checkbox" /><t:label for="centralNorthPacific" class="choice" />
		<t:checkbox t:id="stateOfHawaii" t:value="entity.stateOfHawaii"  class="element checkbox indent" /><t:label for="stateOfHawaii" class="indent" />
		<t:checkbox t:id="northWestHawaiianIslands" t:value="entity.northWestHawaiianIslands"  class="element checkbox indent" /><t:label for="northWestHawaiianIslands" class="indent" />
		<t:checkbox t:id="pacificRemoteIslands" t:value="entity.pacificRemoteIslands"  class="element checkbox indent" /><t:label for="pacificRemoteIslands" class="indent" />
		<t:checkbox t:id="westernNorthPacific" t:value="entity.westernNorthPacific"  class="element checkbox" /><t:label for="westernNorthPacific" class="choice" />
		<t:checkbox t:id="cnmi" t:value="entity.cnmi"  class="element checkbox indent" /><t:label for="cnmi" class="indent" />
		<t:checkbox t:id="fsm" t:value="entity.fsm"  class="element checkbox indent" /><t:label for="fsm" class="indent" />
		<t:checkbox t:id="guam" t:value="entity.guam"  class="element checkbox indent" /><t:label for="guam" class="indent" />
		<t:checkbox t:id="palau" t:value="entity.palau"  class="element checkbox indent" /><t:label for="palau" class="indent" />
		<t:checkbox t:id="rmi" t:value="entity.rmi"  class="element checkbox indent" /><t:label for="rmi" class="indent" />
		<t:checkbox t:id="otherWesternNorthPacific" t:value="entity.otherWesternNorthPacific"  class="element checkbox indent" /><t:label for="otherWesternNorthPacific" class="indent" />
		<t:checkbox t:id="southPacific" t:value="entity.southPacific"  class="element checkbox" /><t:label for="southPacific" class="choice" />
		<t:checkbox t:id="americanSamoa" t:value="entity.americanSamoa"  class="element checkbox indent" /><t:label for="americanSamoa" class="indent" />
		<t:checkbox t:id="australia" t:value="entity.australia"  class="element checkbox indent" /><t:label for="australia" class="indent" />
		<t:checkbox t:id="cookIslands" t:value="entity.cookIslands"  class="element checkbox indent" /><t:label for="cookIslands" class="indent" />
		<t:checkbox t:id="fiji" t:value="entity.fiji"  class="element checkbox indent" /><t:label for="fiji" class="indent" />
		<t:checkbox t:id="frenchPolynesia" t:value="entity.frenchPolynesia"  class="element checkbox indent" /><t:label for="frenchPolynesia" class="indent" />
		<t:checkbox t:id="kiribati" t:value="entity.kiribati"  class="element checkbox indent" /><t:label for="kiribati" class="indent" />
		<t:checkbox t:id="newZealand" t:value="entity.newZealand"  class="element checkbox indent" /><t:label for="newZealand" class="indent" />
		<t:checkbox t:id="png" t:value="entity.png"  class="element checkbox indent" /><t:label for="png" class="indent" />
		<t:checkbox t:id="samoa" t:value="entity.samoa"  class="element checkbox indent" /><t:label for="samoa" class="indent" />
		<t:checkbox t:id="solomonIslands" t:value="entity.solomonIslands"  class="element checkbox indent" /><t:label for="solomonIslands" class="indent" />
		<t:checkbox t:id="tonga" t:value="entity.tonga"  class="element checkbox indent" /><t:label for="tonga" class="indent" />
		<t:checkbox t:id="tuvalu" t:value="entity.tuvalu"  class="element checkbox indent" /><t:label for="tuvalu" class="indent" />
		<t:checkbox t:id="vanuatu" t:value="entity.vanuatu"  class="element checkbox indent" /><t:label for="vanuatu" class="indent" />
		<t:checkbox t:id="otherSouthPacific" t:value="entity.otherSouthPacific"  class="element checkbox indent" /><t:label for="otherSouthPacific" class="indent" />
		<t:checkbox t:id="pacificBasin" t:value="entity.pacificBasin"  class="element checkbox" /><t:label for="pacificBasin" class="choice" />
		<t:checkbox t:id="global" t:value="entity.global"  class="element checkbox" /><t:label for="global" class="choice" />
	
	  </div>
	 </fieldset>
	</li>	



     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('dpBox');" title="Show or Hide">Data - Physical</legend>
       <div id="dpBox" class="unhidden">
		<t:checkbox t:id="dataPhysical" t:value="entity.dataPhysical"  class="element checkbox" /><t:label for="dataPhysical" class="choice" />
		<t:checkbox t:id="insitu" t:value="entity.insitu"  class="element checkbox indent" /><t:label for="insitu" class="indent" />
		<t:checkbox t:id="remote" t:value="entity.remote"  class="element checkbox indent" /><t:label for="remote" class="indent" />
		<t:checkbox t:id="model" t:value="entity.model"  class="element checkbox indent" /><t:label for="model" class="indent" />
		<t:checkbox t:id="reanalysis" t:value="entity.reanalysis"  class="element checkbox indent" /><t:label for="reanalysis" class="indent" />
		<t:checkbox t:id="topo" t:value="entity.topo"  class="element checkbox indent" /><t:label for="topo" class="indent" />
		<t:checkbox t:id="imagery" t:value="entity.imagery"  class="element checkbox indent" /><t:label for="imagery" class="indent" />

	     <fieldset>
	      <legend class="description">Essential Climate Variable - Data</legend>
			<t:checkbox t:id="atmosphericData" t:value="entity.atmosphericData"  class="element checkbox" /><t:label for="atmosphericData" class="choice" />
			<t:checkbox t:id="oceanicData" t:value="entity.oceanicData"  class="element checkbox" /><t:label for="oceanicData" class="choice" />
			<t:checkbox t:id="terrestrialData" t:value="entity.terrestrialData"  class="element checkbox" /><t:label for="terrestrialData" class="choice" />
	      </fieldset>
        </div>
      </fieldset>
     </li>

     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('prBox');" title="Show or Hide">Products - Physical</legend>
       <div id="prBox" class="unhidden">
		<t:checkbox t:id="productsPhysical" t:value="entity.productsPhysical"  class="element checkbox" /><t:label for="productsPhysical" class="choice" />
		<t:checkbox t:id="hindcasts" t:value="entity.hindcasts"  class="element checkbox" /><t:label for="hindcasts" class="choice" />
		<t:checkbox t:id="outlooks" t:value="entity.outlooks"  class="element checkbox" /><t:label for="outlooks" class="choice" />
		<t:checkbox t:id="impactOutlooks" t:value="entity.impactOutlooks"  class="element checkbox indent" /><t:label for="impactOutlooks" class="indent" />
		<t:checkbox t:id="drought" t:value="entity.drought"  class="element checkbox indent2" /><t:label for="drought" class="indent2" />
		<t:checkbox t:id="flooding" t:value="entity.flooding"  class="element checkbox indent2" /><t:label for="flooding" class="indent2" />
		<t:checkbox t:id="erosion" t:value="entity.erosion"  class="element checkbox indent2" /><t:label for="erosion" class="indent2" />
		<t:checkbox t:id="bleaching" t:value="entity.bleaching"  class="element checkbox indent2" /><t:label for="bleaching" class="indent2" />
		<t:checkbox t:id="spatial" t:value="entity.spatial"  class="element checkbox indent" /><t:label for="spatial" class="indent" />
		<t:checkbox t:id="regionNation" t:value="entity.regionNation"  class="element checkbox indent2" /><t:label for="regionNation" class="indent2" />
		<t:checkbox t:id="locationSite" t:value="entity.locationSite"  class="element checkbox indent2" /><t:label for="locationSite" class="indent2" />
		<t:checkbox t:id="timeScale" t:value="entity.timeScale"  class="element checkbox indent" /><t:label for="timeScale" class="indent" />
		<t:checkbox t:id="past" t:value="entity.past"  class="element checkbox indent2" /><t:label for="past" class="indent2" />
		<t:checkbox t:id="current" t:value="entity.current"  class="element checkbox indent2" /><t:label for="current" class="indent2" />
		<t:checkbox t:id="future" t:value="entity.future"  class="element checkbox indent2" /><t:label for="future" class="indent2" />
		<t:checkbox t:id="methodology" t:value="entity.methodology"  class="element checkbox indent" /><t:label for="methodology" class="indent" />
		<t:checkbox t:id="obsInsitu" t:value="entity.obsInsitu"  class="element checkbox indent2" /><t:label for="obsInsitu" class="indent2" />
		<t:checkbox t:id="obsRemote" t:value="entity.obsRemote"  class="element checkbox indent2" /><t:label for="obsRemote" class="indent2" />
		<t:checkbox t:id="modelStatic" t:value="entity.modelStatic"  class="element checkbox indent2" /><t:label for="modelStatic" class="indent2" />
		<t:checkbox t:id="modelDynamic" t:value="entity.modelDynamic"  class="element checkbox indent2" /><t:label for="modelDynamic" class="indent2" />
		<t:checkbox t:id="projections" t:value="entity.projections"  class="element checkbox" /><t:label for="projections" class="choice" />
		<t:checkbox t:id="guidance" t:value="entity.guidance"  class="element checkbox" /><t:label for="guidance" class="choice" />
		<t:checkbox t:id="applications" t:value="entity.applications"  class="element checkbox" /><t:label for="applications" class="choice" />

       

	     <fieldset>
	      <legend class="description">Essential Climate Variable - Products</legend>
	      <t:checkbox t:id="atmosphericProducts" t:value="entity.atmosphericProducts"  class="element checkbox" /><t:label for="atmosphericProducts" class="choice" />
           <t:checkbox t:id="oceanicProducts" t:value="entity.oceanicProducts"  class="element checkbox" /><t:label for="oceanicProducts" class="choice" />
           <t:checkbox t:id="terrestrialProducts" t:value="entity.terrestrialProducts"  class="element checkbox" /><t:label for="terrestrialProducts" class="choice" />
	      </fieldset>
	    </div>
      </fieldset>
     </li>


     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('npBox');" title="Show or Hide">Data, Products, and Tools - Non-physical</legend>
       <div id="npBox" class="unhidden">

		<t:checkbox t:id="nonPhysical" t:value="entity.nonPhysical"  class="element checkbox" /><t:label for="nonPhysical" class="choice" />
		<t:checkbox t:id="biological" t:value="entity.biological"  class="element checkbox indent" /><t:label for="biological" class="indent" />
		<t:checkbox t:id="socioEconomic" t:value="entity.socioEconomic"  class="element checkbox indent" /><t:label for="socioEconomic" class="indent" />
		<t:checkbox t:id="cultural" t:value="entity.cultural"  class="element checkbox indent" /><t:label for="cultural" class="indent" />
		<t:checkbox t:id="otherNonPhysical" t:value="entity.otherNonPhysical"  class="element checkbox indent" /><t:label for="otherNonPhysical" class="indent" />


       </div>
       
      </fieldset>
     </li>




     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('sectorBox');" title="Show or Hide">Sector</legend>
       <div id="sectorBox" class="unhidden">

		<t:checkbox t:id="health" t:value="entity.health"  class="element checkbox" /><t:label for="health" class="choice" />
		<t:checkbox t:id="freshWater" t:value="entity.freshWater"  class="element checkbox" /><t:label for="freshWater" class="choice" />
		<t:checkbox t:id="energy" t:value="entity.energy"  class="element checkbox" /><t:label for="energy" class="choice" />
		<t:checkbox t:id="transportation" t:value="entity.transportation"  class="element checkbox" /><t:label for="transportation" class="choice" />
		<t:checkbox t:id="planning" t:value="entity.planning"  class="element checkbox" /><t:label for="planning" class="choice" />
		<t:checkbox t:id="socioCultural" t:value="entity.socioCultural"  class="element checkbox" /><t:label for="socioCultural" class="choice" />
		<t:checkbox t:id="agriculture" t:value="entity.agriculture"  class="element checkbox" /><t:label for="agriculture" class="choice" />
		<t:checkbox t:id="recreation" t:value="entity.recreation"  class="element checkbox" /><t:label for="recreation" class="choice" />
		<t:checkbox t:id="ecological" t:value="entity.ecological"  class="element checkbox" /><t:label for="ecological" class="choice" />
		<t:checkbox t:id="otherSector" t:value="entity.otherSector"  class="element checkbox" /><t:label for="otherSector" class="choice" />
		


       </div>
       
      </fieldset>
     </li>


	

	<li id="li_description">
	<t:label class="description" for="description" />
	<t:textArea t:id="description" t:validate="maxlength=4096" rows="5" cols="90" value="entity.description" />
	</li>
    <li id="li_url">
	<t:label class="description" for="url" />
	<t:textArea t:id="url" t:validate="maxlength=255" rows="1" cols="90" value="entity.url" />
	</li>
     <li id="li_agency">
      <t:label class="description" for="organization" />
      <div>
       <t:textArea t:id="organization" t:validate="maxlength=2048" rows="3" cols="90" value="entity.organization" />
      </div>
     </li>
     <li id="li_contact">
      <t:label class="description" for="contact" />
      <div>
       <t:textArea t:id="contact" t:validate="maxlength=2048" rows="3" cols="90" value="entity.contact" />
      </div>
     </li>
	<li id="li_keywords">
	<t:label class="description" for="keywords" />
	<t:textArea t:id="keywords" t:validate="maxlength=4096" rows="5" cols="90" value="entity.keywords" />
	</li>
	<li id="li_worksheet">
	<t:label class="description" for="worksheet" />
	<t:textArea t:id="worksheet" t:validate="maxlength=255" rows="3" cols="90" value="entity.worksheet" />
	</li>


   </ul>
  </t:form>	
    
 </body>
</html>